<Window x:Class="WallpaperSwapperUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="180" Width="240">
    <Grid>
        <Button x:Name="SetWallpaperButton" Content="Set Wallpaper" HorizontalAlignment="Center" Margin="0,35,100,0" VerticalAlignment="Top" Click="SetWallpaperButton_Click" RenderTransformOrigin="-0.316,0.705" Padding="5,1"/>
        <Button x:Name="RestoreButton" Content="Restore" HorizontalAlignment="Center" Margin="100,35,0,0" VerticalAlignment="Top" Click="RestoreButton_Click" Padding="5,1" Width="75"/>
        <Button x:Name="RunSwapperButton" Content="Run Swapper" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Top" Padding="5,1" Click="RunSwapperButton_Click"/>

    </Grid>
</Window>
